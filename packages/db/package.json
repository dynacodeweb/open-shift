{
  "name": "@workspace/db",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "db:generate": "pnpm exec drizzle-kit generate",
    "db:migrate:up": "pnpm exec drizzle-kit migrate",
    "db:push": "pnpm exec drizzle-kit push",
    "db:studio": "npx drizzle-kit studio"
  },
  "dependencies": {
    "@neondatabase/serverless": "1.0.2",
    "dotenv": "17.2.3",
    "drizzle-orm": "^0.45.1",
    "drizzle-zod": "0.8.3",
    "pg": "8.16.3"
  },
  "devDependencies": {
    "@types/node": "25.0.3",
    "@types/pg": "8.16.0",
    "drizzle-kit": "0.31.8",
    "typescript": "latest"
  },
  "exports": {
    "./auth-db": {
      "types": "./dist/auth-db/index.d.ts",
      "default": "./dist/auth-db/index.js"
    },
    "./auth-types": {
      "types": "./dist/auth-types/index.d.ts",
      "default": "./dist/auth-types/index.js"
    }
  }
}